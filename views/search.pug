extends layouts/main
include mixins/videoBlock

block content
  .search__header
    h3 Searching for #{searchingWord}
  .search__videos
    if videosList.length === 0
      h5 No Videos Found
    each item in videosList
      +videoBlock({
        id : item.id,
        title : item.title,
        views : item.views,
        fileUrl : item.fileUrl
      })